function pws_setCookie(name,value,path,domain,secure,expires){document.cookie=name+'='+escape(value)+
((path)?'; path='+path:'')+
((domain)?'; domain='+domain:'')+
((secure)?';secure':'')+
((expires)?';expires='+expires.toGMTString():'');}